{
  "name": "react-router-prompt",
  "version": "0.0.3",
  "description": "React Router Navigation Prompt for v6",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "author": "Shyam Gupta (shyamm@outlook.com)",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sshyam-gupta/react-router-prompt.git"
  },
  "homepage": "https://github.com/sshyam-gupta/react-router-prompt#readme",
  "keywords": [
    "confirm",
    "navigation",
    "prompt",
    "react",
    "router"
  ],
  "scripts": {
    "build": "yarn build:esm && yarn build:cjs",
    "build:esm": "rimraf dist && tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "prepublishOnly": "yarn build",
    "rollup": "rimraf dist && rollup -c"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.3.1",
    "@types/react": "^17.0.40",
    "@types/react-dom": "^17.0.13",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "eslint": "^8.11.0",
    "eslint-plugin-react": "^7.29.4",
    "rimraf": "^3.0.2",
    "rollup": "^2.70.1",
    "rollup-plugin-dts": "^4.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.6.2"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "react-router": ">=6.x",
    "react-router-dom": ">=6.x"
  },
  "files": [
    "dist"
  ],
  "dependencies": {}
}
